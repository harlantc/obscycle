<?xml version="1.0" encoding="us-ascii"?>

<!DOCTYPE formView SYSTEM "formView.dtd">

<formView>

  <title>HRC Form</title>

  <info>
    <supportsNew>clone null noDelete</supportsNew>
  </info>

  <key>
    <tableName>hrcparam</tableName>
    <columnName>hrcid</columnName>
  </key>

  <navigation>
    <display>
      <name>HRC Identifier</name>
      <tableName>hrcparam</tableName>
      <columnName>hrcid</columnName>
    </display>
    <display>
      <name>HRC SI Mode</name>
      <tableName>hrcparam</tableName>
      <columnName>si_mode</columnName>
    </display>
  </navigation>

  <GUI>
    <label tag="11">
      <labelText>HRC</labelText>
      <row>
        <number>0</number>
      </row>
      <column>
        <number>0</number>
      </column>
    </label>

    <textfield tag="12">
      <labelText>SI Mode</labelText>
      <size>10</size>
      <row>
        <number>+</number>
      </row>
      <column>
        <number>0</number>
      </column>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>si_mode</columnName>
      </database>
    </textfield>

    <textfield tag="21">
      <labelText>Trigger Level</labelText>
      <size>3</size>
      <row>
        <number>+</number>
      </row>
      <column>
        <number>0</number>
      </column>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>trigger_level</columnName>
      </database>
    </textfield>

    <textfield tag="22">
      <labelText>Range Switch Level</labelText>
      <size>3</size>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>range_switch_level</columnName>
      </database>
    </textfield>

    <combobox tag="23">
      <labelText>Spect Mode</labelText>
      <size>8</size>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>spect_mode</columnName>
      </database>
    </combobox>

    <combobox tag="41">
      <labelText>Antico Enable</labelText>
      <size>1</size>
      <row>
        <number>+</number>
      </row>
      <column>
        <number>0</number>
      </column>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>antico_enable</columnName>
      </database>
    </combobox>

    <combobox tag="42">
      <labelText>Width Enable</labelText>
      <size>1</size>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>width_enable</columnName>
      </database>
    </combobox>

    <textfield tag="43">
      <labelText>Width Threshold</labelText>
      <size>3</size>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>width_threshold</columnName>
