    <triplebox tag="91">
      <labelText>Zero block</labelText>
      <row>
        <number>+</number>
      </row>
      <column>
        <number>0</number>
      </column>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>hrc_zero_block</columnName>
      </database>
    </triplebox>

    <triplebox tag="92">
      <labelText>Timing Mode</labelText>
      <database>
        <tableName>hrcparam</tableName>
	<columnName>timing_mode</columnName>
      </database>
    </triplebox>


 </GUI>

</formView>
